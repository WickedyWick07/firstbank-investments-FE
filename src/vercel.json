{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://firstbank-investsmart.herokuapp.com/api/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://firstbank-investsmart.herokuapp.com/api/$1"
      }
    ]
  }
  