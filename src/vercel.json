{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vitejs/plugin-react"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }